	and prove</para>
  

    <rule id="proposition3" type="proposition">
      <statement id="idp2104832">
	<para id="prop3eq"> If <m:math>
	    <m:apply>
	    <m:leq/>
	    <m:cn>1</m:cn>
	    <m:ci>k</m:ci>
