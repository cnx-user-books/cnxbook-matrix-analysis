		  <m:domainofapplication>
		    <m:apply>
		      <m:diff/>
		      <m:ci><m:msub>
			  <m:mi>C</m:mi><m:mi>j</m:mi>
			</m:msub></m:ci>
		    </m:apply>
		  </m:domainofapplication>
		  <m:apply>
		    <m:times/>
		    <m:apply>
		      <m:ci type="fn">R</m:ci>
