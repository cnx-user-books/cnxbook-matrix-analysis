		    </m:apply>
		    <m:apply>
		      <m:ci type="fn">R</m:ci>
		      <m:ci>w</m:ci>
		    </m:apply>
		    <m:apply>
		      <m:power/>
		      <m:apply>
			<m:minus/>
			<m:ci>z</m:ci>
			<m:ci><m:msub>
			    <m:mi>λ</m:mi><m:mi>j</m:mi>
			  </m:msub></m:ci>
		      </m:apply>
		      <m:ci>k</m:ci>
		    </m:apply>
		    <m:apply>
		      <m:power/>
		      <m:apply>
			<m:minus/>
